{
  "name": "webpacktemplate",
  "version": "1.0.0",
  "description": "My template",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --open --config ./build/webpack.dev.conf.js",
    "build": "webpack --config ./build/webpack.build.conf.js",
    "lint": "node node_modules/eslint/bin/eslint.js ./src",
    "fix": "eslint --fix src/",
    "coverage:full": "jest --coverage --coverage-reporters=lcov",
    "test": "jest"
  },
  "browserslist": [
    "> 1%",
    "last 3 version"
  ],
  "author": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.7.4",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "css-mqpacker": "^7.0.0",
    "css-url-relative-plugin": "^1.0.0",
    "cssnano": "^4.1.10",
    "enzyme-adapter-react-15": "^1.4.1",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "file-loader": "^4.0.0",
    "history": "^4.10.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "materialize-css": "^1.0.0-rc.2",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.13.0",
    "postcss-loader": "^3.0.0",
    "sass-jest": "^0.1.7",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1",
    "enzyme": "^3.11.0",
    "firebase": "^7.7.0",
    "jest": "^24.9.0"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-test-renderer": "^16.12.0",
    "resolve-url-loader": "^3.1.1"
  },
  "jest": {
    "moduleNameMapper": {
      "@markoffice/rutoken": "<rootDir>/src/utils/rutoken/rutoken.local.js",
      "@markoffice/data-provider": "<rootDir>/src/api/provider.api.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/config/setupTest.js",
    "coverageReporters": [
      "html"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "jest-canvas-mock"
    ]
  }
}
